<% if other_user.exists? %>
<table class="table">
  <tbody>
    <tr>
      <td>image</td>
      <td>name</td>
      <td colspan="3"></td>
    </tr>
    <% other_user.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(80,80) %></td>
        <td><%= user.name %></td>
        <td><p>フォロー数:<%= user.following.count %></p></td>
        <td><p>フォロワー数:<%= user.followers.count %></p></td>
        <td><%= link_to "Show",user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% else %>
<p>ユーザーはいません</p>
<% end %>